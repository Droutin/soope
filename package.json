{
    "name": "soope",
    "version": "0.0.2",
    "author": "Roman Va≈°ek",
    "license": "MIT",
    "repository": {
        "url": "https://github.com/Droutin/soope"
    },
    "main": "./dist/cjs/index.js",
    "exports": {
        ".": "./dist/esm/index.js",
        "./decorators": "./dist/esm/decorators",
        "./types": "./dist/esm/types",
        "./utils": "./dist/esm/utils"
    },
    "module": "./dist/esm/index.js",
    "types": "./dist/types/index.d.js",
    "scripts": {
        "build": "yarn build:cjs && yarn build:esm && yarn build:types",
        "link": "yarn link",
        "build:cjs": "rm -rf ./dist/cjs && tsc -p config/tsconfig.cjs.json",
        "build:esm": "rm -rf ./dist/esm && tsc -p config/tsconfig.esm.json",
        "build:types": "rm -rf ./dist/types && tsc -p config/tsconfig.types.json",
        "lint": "eslint . --ext .ts",
        "prettier": "prettier --config .prettierrc . --write"
    },
    "dependencies": {
        "dotenv": "^16.0.2",
        "express": "^4.18.1"
    },
    "devDependencies": {
        "@types/express": "^4.17.14",
        "@typescript-eslint/eslint-plugin": "^5.38.0",
        "@typescript-eslint/parser": "^5.38.0",
        "eslint": "^8.23.1",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-prettier": "^4.2.1",
        "prettier": "^2.7.1",
        "typescript": "^4.8.3"
    }
}
